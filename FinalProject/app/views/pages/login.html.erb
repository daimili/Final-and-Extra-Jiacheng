<h1> Social Sharing Site </h1>

<%= form_tag sessions_url do %>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>

  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <%= submit_tag "Sign In" %> <%= link_to "Sign Up", new_user_url, class: 'btn btn-primary' %>
<% end %>
